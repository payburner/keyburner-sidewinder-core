    "@typescript-eslint/parser": "^2.27.0",
    "assert-diff": "^3.0.0",
    "doctoc": "^1.4.0",
    "ejs": "^3.0.1",
    "eslint": "^6.5.1",
    "eventemitter2": "^6.0.0",
    "jest": "^26.1.0",
    "jest-config": "^26.1.0",
    "json-schema-to-markdown-table": "^0.4.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.4.1",
    "tslint": "^6.1.3",
    "typescript": "^3.7.5",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9"
